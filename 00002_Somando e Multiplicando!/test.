describe("Exercício 2", function() {
	it("Operar Array", function() {
	  assert(typeof soma !== "undefined", "Falta a variável soma...")
	  
	  assert(soma === somatorio(miArray), "A variável soma não tem o resultado esperado")
	  
	  assert(typeof mult !== "undefined", "Falta a variável mult...")
	  
	  assert(mult === produto(meuArray), "A variável mult não tem o resultado esperado")
	  
	  assert(pasePorOperar, "Você chamou a função operarArray?")
	})
  })